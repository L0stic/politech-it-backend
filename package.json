{
  "name": "poly-js",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "xo": {
    "ignores": [
      "db/seeds"
    ],
    "rules": {
      "new-cap": [
        "error",
        {
          "capIsNewExceptions": [
            "Router"
          ]
        }
      ]
    }
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "db:init": "npx knex migrate:latest --knexfile ./db/knexfile.js && npx knex seed:run --knexfile ./db/knexfile.js",
    "db:rollback": "npx knex migrate:rollback --all --knexfile ./db/knexfile.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/L0stic/poly-js.git"
  },
  "keywords": [],
  "author": "Lostic",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/L0stic/poly-js/issues"
  },
  "homepage": "https://github.com/L0stic/poly-js#readme",
  "dependencies": {
    "express": "^4.17.3",
    "joi": "^17.6.0",
    "knex": "^1.0.4",
    "password-hash": "^1.2.2",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "xo": "^0.48.0"
  }
}
